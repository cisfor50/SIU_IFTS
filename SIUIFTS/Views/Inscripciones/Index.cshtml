@model IEnumerable<SIUIFTS.Models.Inscripcion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AlumnoID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Materia)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AlumnoID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Materia.MateriaID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InscripcionID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InscripcionID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InscripcionID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
